<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    coreApp="true"
    package="com.tzj.providers"
    android:sharedUserId="android.uid.system">

    <permission
        android:name="com.tzj.providers.permission.WRITE"
        android:label="write memory_provider"
        android:protectionLevel="normal" />

    <application
        android:name=".App"
        android:allowClearUserData="false"
        android:defaultToDeviceProtectedStorage="true"
        android:directBootAware="true"
        android:killAfterRestore="false"
        android:label="@string/app_name"
        android:process="system"
        android:restoreAnyVersion="true">

        <provider
            android:name="MemoryProvider"
            android:authorities="memory_provider"
            android:exported="true"
            android:initOrder="99"
            android:multiprocess="false"
            android:singleUser="true"
            android:visibleToInstantApps="true"
            android:writePermission="com.tzj.providers.permission.WRITE" />
    </application>
</manifest>
